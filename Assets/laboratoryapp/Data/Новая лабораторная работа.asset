    \u0440\u0430\u0432\u0435\u043D \u043E\u0431\u044A\u0451\u043C \u043D\u0430\u043B\u0438\u0442\u043E\u0439
    \u0432\u043E\u0434\u044B. \u0412\u043C\u0435\u0441\u0442\u0438\u043C\u043E\u0441\u0442\u044C
    \u0441\u0442\u0430\u043A\u0430\u043D\u0430 \u0431\u0443\u0434\u0435\u0442 \u0442\u0430\u043A\u043E\u0439
    \u0436\u0435.\r\n6. \u0422\u0430\u043A\u0438\u043C \u0436\u0435 \u043E\u0431\u0440\u0430\u0437\u043E\u043C
    \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0438\u0442\u0435 \u0432\u043C\u0435\u0441\u0442\u0438\u043C\u043E\u0441\u0442\u044C
